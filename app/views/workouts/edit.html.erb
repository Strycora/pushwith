<h3> Edit Workout </h3>

<%= form_for @workout do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :sets %>
    <%= f.number_field :sets %>
  </div>
  <div>
    <%= f.label :reps %>
    <%= f.number_field :reps %>
  </div>

  <%= f.submit "Update Workout" %>
  <% end %>