<h1>Workout List for <%=@workout_group.name%></h1>

<ul>
  <% @workouts.each do |workout| %>
    <li><%= link_to workout.name, workout_group_workout_path(@workout_group, workout) %></li>
  <% end %>
</ul>

<%= link_to "Create Workout for #{@workout_group.name}", new_workout_group_workout_path%>
<br>
<%= link_to "Workout Group", workout_group_path(@workout_group) %>